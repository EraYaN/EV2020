<Window x:Class="ASIOTest.PlotWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:oxy="http://oxyplot.codeplex.com"
		xmlns:local="clr-namespace:ASIOTest"
        Title="PlotWindow" Height="450" Width="800" WindowStartupLocation="CenterScreen">
	<Window.DataContext>
		<local:Presenter/>
	</Window.DataContext>
	<Grid>		
		<oxy:Plot Model="{Binding PlotModel, UpdateSourceTrigger=PropertyChanged}"></oxy:Plot>
	</Grid>
</Window>
